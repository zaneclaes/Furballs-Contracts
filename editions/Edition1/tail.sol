pragma solidity ^0.8.2;

import "../IFurballPart.sol";

/// @author LFG Gaming LLC
contract FurballsEdition1tail is IFurballPart {
  mapping(uint8 => uint8[]) private _options;

  constructor() {
    _options[0].push(0);
    _options[0].push(1);
    _options[0].push(2);
    _options[0].push(3);
    _options[0].push(4);
    _options[0].push(5);
    _options[0].push(6);
    _options[3].push(7);
    _options[0].push(8);
    _options[2].push(9);
    _options[0].push(10);
  }

  function slot() external pure override returns(string memory) {
    return "Tail";
  }

  function options(uint8 rarity) external view override returns(uint8[] memory) {
    return _options[rarity];
  }

  function name(uint8 idx) external pure override returns(string memory) {
    if (idx == 0) return "Dinosaur";
    if (idx == 1) return "Cat";
    if (idx == 2) return "Lion";
    if (idx == 3) return "Rabbit";
    if (idx == 4) return "Wolf";
    if (idx == 5) return "Rat";
    if (idx == 6) return "Fox";
    if (idx == 7) return "Unicorn";
    if (idx == 8) return "Poodle";
    if (idx == 9) return "Sprinkle Pony";
    if (idx == 10) return "Racoon";
    return "";
  }

  function count() external pure override returns(uint8) {
    return uint8(11);
  }

  function data() external pure override returns(bytes memory) {
    return hex"";
  }
}