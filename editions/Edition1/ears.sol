pragma solidity ^0.8.2;

import "../IFurballPart.sol";

/// @author LFG Gaming LLC
contract FurballsEdition1ears is IFurballPart {
  mapping(uint8 => uint8[]) private _options;

  constructor() {
    _options[0].push(0);
    _options[0].push(1);
    _options[0].push(2);
    _options[0].push(3);
    _options[0].push(4);
    _options[1].push(5);
    _options[0].push(6);
    _options[0].push(7);
    _options[0].push(8);
    _options[0].push(9);
  }

  function slot() external pure override returns(string memory) {
    return "Ears";
  }

  function options(uint8 rarity) external view override returns(uint8[] memory) {
    return _options[rarity];
  }

  function name(uint8 idx) external pure override returns(string memory) {
    if (idx == 0) return "Corgi";
    if (idx == 1) return "Cat";
    if (idx == 2) return "Gremlin";
    if (idx == 3) return "Koala";
    if (idx == 4) return "Bunny";
    if (idx == 5) return "Stag";
    if (idx == 6) return "Bear";
    if (idx == 7) return "Gorilla";
    if (idx == 8) return "Dog";
    if (idx == 9) return "Llama";
    return "";
  }

  function count() external pure override returns(uint8) {
    return uint8(10);
  }

  function data() external pure override returns(bytes memory) {
    return hex"";
  }
}