pragma solidity ^0.8.2;

import "../IFurballPart.sol";

/// @author LFG Gaming LLC
contract FurballsEdition1neck is IFurballPart {
  mapping(uint8 => uint8[]) private _options;

  constructor() {
    _options[0].push(0);
    _options[0].push(1);
    _options[0].push(2);
    _options[0].push(3);
    _options[0].push(4);
    _options[0].push(5);
    _options[0].push(6);
    _options[1].push(7);
    _options[0].push(8);
    _options[1].push(9);
    _options[3].push(10);
    _options[2].push(12);

    // Patch: Furdenza
    _options[3].push(11);
  }

  function slot() external pure override returns(string memory) {
    return "Outfit";
  }

  function options(uint8 rarity) external view override returns(uint8[] memory) {
    return _options[rarity];
  }

  function name(uint8 idx) external pure override returns(string memory) {
    if (idx == 0) return "Dapper Shirt";
    if (idx == 1) return "Triple Chin";
    if (idx == 2) return "Chin";
    if (idx == 3) return "Spiked Collar";
    if (idx == 4) return "Bowtie";
    if (idx == 5) return "Cone of Shame";
    if (idx == 6) return "Pirate";
    if (idx == 7) return "Fur";
    if (idx == 8) return "Dog Tag";
    if (idx == 9) return "ET Hoodie";
    if (idx == 10) return "Grey Hoodie";
    if (idx == 11) return "Furdenza Hoodie";
    if (idx == 12) return "Royal Cape";
    return "";
  }

  function count() external pure override returns(uint8) {
    return uint8(13);
  }

  function data() external pure override returns(bytes memory) {
    return hex"";
  }
}